{
  "name": "@tawdig/tempus",
  "version": "1.1.0",
  "author": {
    "name": "Travis Aaron Wagner",
    "email": "travis@travisaw.com"
  },
  "bugs": {
    "url": "https://github.com/trvswgnr/tempus/issues"
  },
  "description": "Date and time management simplified",
  "homepage": "https://github.com/trvswgnr/tempus",
  "keywords": [
    "date",
    "time",
    "js",
    "javascript"
  ],
  "license": "MIT",
  "main": "dist/index.js",
  "module": "src/index.js",
  "jsdelivr": "dist/index.bundle.min.js",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/trvswgnr/tempus.git"
  },
  "scripts": {
    "build": "webpack --mode=production --config webpack.module.js && webpack --mode=production --config webpack.cdn.js",
    "start": "webpack --watch --mode=production --config webpack.module.js",
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "eslint src --ext .js"
  },
  "bundleDependencies": false,
  "funding": "https://github.com/sponsors/trvswgnr",
  "devDependencies": {
    "@babel/core": "^7.17.8",
    "@babel/preset-env": "^7.16.11",
    "babel-loader": "^8.2.4",
    "eslint": "^8.13.0",
    "eslint-config-xo": "^0.40.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-unicorn": "^41.0.0",
    "prettier": "^2.6.2",
    "terser-webpack-plugin": "^5.2.4",
    "webpack": "^5.56.1",
    "webpack-cli": "^4.8.0",
    "webpack-merge": "^5.8.0"
  },
  "browserslist": [
    ">= 0.5%",
    "last 2 major versions",
    "not dead",
    "Chrome >= 60",
    "Firefox >= 60",
    "Firefox ESR",
    "iOS >= 12",
    "Safari >= 12",
    "not Explorer <= 11"
  ],
  "files": [
    "dist/**/*",
    "src/**/*"
  ],
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
